<h1>List of Prehistoric Creatures</h1>
<form method='GET' action='/prehistoriccreatures'>
    <label for='nameFilter'>Filter by Name:</label>
    <input type='text' id='nameFilter' name='nameFilter'>
    <input type='submit'>
</form>
<ul>
    <% pCreats.forEach(creat=>{ %>
        <li><img src='<%= creat.img_url%>'> is a <%= creat.type %></li>
    <%})%>
</ul>